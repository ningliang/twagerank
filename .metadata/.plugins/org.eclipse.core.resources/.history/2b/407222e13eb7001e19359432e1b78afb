package analysis;

import it.unimi.dsi.fastutil.ints.Int2DoubleOpenHashMap;

import java.io.BufferedInputStream;
import java.io.DataInputStream;
import java.io.FileInputStream;
import java.io.IOException;

public class SortPrint {
	public static void main(String[] args) {
		// If we have a starting guess read it in over current state
		Int2DoubleOpenHashMap state = null;
		if (args.length == 3) {
			System.out.println("Getting initial guess from file");
			DataInputStream in = null;
			state = new Int2DoubleOpenHashMap();
			try {
				in = new DataInputStream(new BufferedInputStream(new FileInputStream(args[0])));
				while (in.available() > 0) {
					int key = in.readInt();
					double value = in.readDouble();
					state.put(key, value);
				}
			} catch (IOException e) {
				e.printStackTrace();
				System.exit(1);
			} finally {
				if (in != null) {
					try { in.close();
					} catch (IOException e) { e.printStackTrace(); }
				}
			}
			System.out.println("Read " + state.size() + " keys from guess.");
		}
	}
}
